{
  "name": "medical-appointment-system",
  "version": "1.0.0",
  "description": "Medical appointment scheduling system for insured patients (PE/CL)",
  "type": "module",
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:unit": "NODE_OPTIONS=--experimental-vm-modules jest --testPathPattern=tests/core",
    "deploy": "serverless deploy",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:prod": "serverless deploy --stage prod",
    "remove": "serverless remove",
    "remove:dev": "serverless remove --stage dev",
    "remove:prod": "serverless remove --stage prod",
    "logs": "serverless logs -f",
    "logs:create": "serverless logs -f createAppointment --tail",
    "logs:list": "serverless logs -f listAppointments --tail",
    "logs:pe": "serverless logs -f appointmentPE --tail",
    "logs:cl": "serverless logs -f appointmentCL --tail",
    "logs:updater": "serverless logs -f appointmentStatusUpdater --tail",
    "local": "serverless offline start"
  },
  "keywords": [
    "serverless",
    "aws",
    "lambda",
    "medical",
    "appointments",
    "clean-architecture",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.645.0",
    "@aws-sdk/client-eventbridge": "^3.645.0",
    "@aws-sdk/client-sns": "^3.645.0",
    "@aws-sdk/lib-dynamodb": "^3.645.0",
    "mysql2": "^3.11.0",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.4",
    "@types/uuid": "^10.0.0",
    "dotenv": "^17.2.3",
    "esbuild": "^0.23.1",
    "jest": "^29.7.0",
    "serverless": "^4.4.3",
    "serverless-esbuild": "^1.54.3",
    "serverless-offline": "^14.2.0",
    "swagger-ui-express": "^5.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  }
}
